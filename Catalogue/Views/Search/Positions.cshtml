<table class="table">
    <tr>
        <th>
            Наименование должности
        </th>
        <th></th>
    </tr>

    @foreach (var position in ViewBag.Positions)
    {
        <tr>
            <td>
                @position.PositionName
            </td>
            <td>
                <a class="btn btn-default" name="editButton" id="editButton" href="@Url.Action("Edit","Position", new {id = position.PositionId})">Редактировать</a> |
                <a class="btn btn-default" name="detailsButton" id="detailsButton" href="@Url.Action("Details","Position", new {id = position.PositionId})">Просмотреть</a> |
                <a class="btn btn-default" name="deleteButton" id="deleteButton" href="@Url.Action("Delete","Position", new {id = position.PositionId})">Удалить</a>
            </td>
        </tr>
    }

</table>
<button class="btn btn-default" onclick="toPrevMain()">Назад</button>
