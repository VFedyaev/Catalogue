<table class="table">
    <tr>
        <th>
            Орган управления
        </th>
        <th>
            Отдел
        </th>
        <th></th>
    </tr>

@foreach (var department in ViewBag.Departments) {
    <tr>
        <td>
            @department.Administration.AdministrationName
        </td>
        <td>
            @department.DepartmentName
        </td>
        <td>
            <a class="btn btn-default" name="editButton" id="editButton" href="@Url.Action("Edit","Department", new {id = department.DepartmentId})">Редактировать</a> |
            <a class="btn btn-default" name="detailsButton" id="detailsButton" href="@Url.Action("Details","Department", new {id = department.DepartmentId})">Просмотреть</a> |
            <a class="btn btn-default" name="deleteButton" id="deleteButton" href="@Url.Action("Delete","Department", new {id = department.DepartmentId})">Удалить</a>
        </td>
    </tr>
}

</table>
<button class="btn btn-default" onclick="toPrevMain()">Назад</button>
